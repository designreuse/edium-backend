{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "edium-service-user",
      "image": "edium/edium-service-user",
      "essential": true,
      "memory": 256,
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        }
      ],
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 8081
        }
      ]
    }
  ]
}